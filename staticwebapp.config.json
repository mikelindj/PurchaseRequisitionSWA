{
  "routes": [
    {
      "route": "/*",
      "allowedRoles": ["authenticated"]
    }
  ],
  "navigationFallback": {
    "rewrite": "/index.html",
    "exclude": ["/images/*.{png,jpg,gif}", "/css/*"]
  },
  "auth": {
    "identityProviders": {
      "azureActiveDirectory": {
        "registration": "YOUR_AAD_APP_REGISTRATION_NAME"
      }
    }
  },
  "responseOverrides": {
    "401": {
      "rewrite": "/index.html",
      "statusCode": 302
    }
  }
}

